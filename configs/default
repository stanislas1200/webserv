server {
    autoindex on
	errorpages 404 www/pages/errorpages/error_404.html
	errorpages 503 www/pages/errorpages/error_503.html
	listen 8080
    methodes GET AGAIN
    server_names littleCloud
    client_size 42
    template www/pages/template.html
    timeoutCgi  10
    root src
    location /about {
        root www/pages/
        redirection about.html
    }
    location /about {
        root ./
        methodes GET
        redirection /pages
    }
    location / {
        methodes GET POST DELETE
        root www/pages/
        redirection index.html
        cgi_path   cgiFiles/
        cgi_extention  .cgi .py sdfdfs
        template www/pages/template.html
    }
    location /about {
        root www/pages/
        redirection about.html
    }
    location /cgi {
        root cgiFiles/
        redirection test.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/test {
        root cgiFiles/
        redirection cgi_tester.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/env {
        root cgiFiles/
        methodes POST GET
        redirection printEnv.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /listUpload {
        root cgiFiles/
        redirection listUpload.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /cgi/post {
        root cgiFiles/
        methodes POST 
        redirection post.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /favicon.ico {
        root www/pages/
        methodes GET
        redirection favicon.ico
    }
    location /errorpages/error_503.html {
        root src/pages/errorpages/
        methodes GET
        redirection error_503.html
    }
    location /download {
        root cgiFiles/
        methodes GET
        redirection getUpload.py
        cgi_path   cgiFiles/
        cgi_extention .py
    }
    location /arrow.png {
        root www/pages/
        methodes GET
        redirection arrow.png
    }
}


server {
    autoindex on
    template www/pages/template.html
	errorpages 505 www/pages/errorpages/error_505.html
    methodes GET POST DELETE
	listen 8081
    timeoutCgi  10
    root src
    location / {
        root www/pages/
        methodes GET POST
        redirection index.html
    }
    location /favicon.ico {
        root www/pages/
        methodes GET
        redirection favicon.ico
    }
    server_names bigCloud
    client_size 43
}

