server {
	errorpages			404 www/pages/errorpages/error_404.html
	errorpages			503 www/pages/errorpages/error_503.html
	listen				8080
	methodes			NONE
	server_names		littleCloud
	client_size			42
    autoindex			on
    timeoutCgi			10
	template			www/pages/template.html
	root				www

	location / {
        root			www/pages/
		methodes		GET POST DELETE
		redirection		index.html
		cgi_path		cgiFiles/
		cgi_extention	.cgi .py
	}
	location /about {
        root			www/pages/
		methodes		GET
		redirection		about.html
	}
	location /cgi {
        root			www/pages/
		methodes		GET
		redirection		cgi.html
	}
    location /favicon.ico {
        root			www/images/
        methodes		GET
        redirection		favicon.ico
    }
    location /arrow.png {
        root			www/images/
        methodes		GET
        redirection		arrow.png
    }
	location /styles.css {
		root			www/css/
		methodes		GET
		redirection		styles.css
	}
	location /script.js {
        root			www/js/
		methodes		GET
		redirection		script.js
	}
	location /cgi/listUpload {
		root			cgiFiles/
		methodes		GET
		redirection		listUpload.py
		cgi_path		cgiFiles/
		cgi_extention	.py
	}
	location /cgi/download {
		root			cgiFiles/
		methodes		GET
		redirection		getUpload.py
		cgi_path		cgiFiles/
		cgi_extention	.py
	}
	location /cgi/helloBash {
		root			cgiFiles/helloWorld/
		methodes		GET
		redirection		helloBash.sh
		cgi_path		cgiFiles/
		cgi_extention	.sh .pl .php .py
	}
	location /cgi/helloPerl {
		root			cgiFiles/helloWorld/
		methodes		GET
		redirection		helloPerl.pl
		cgi_path		cgiFiles/
		cgi_extention	.sh .pl .php .py
	}
	location /cgi/helloPhp {
		root			cgiFiles/helloWorld/
		methodes		GET
		redirection		helloPhp.php
		cgi_path		cgiFiles/
		cgi_extention	.sh .pl .php .py
	}
	location /cgi/helloPython {
		root			cgiFiles/helloWorld/
		methodes		GET
		redirection		helloPython.py
		cgi_path		cgiFiles/
		cgi_extention	.sh .pl .php .py
	}
	location /cgi/env {
		root			cgiFiles/
		methodes		GET
		redirection		printEnv.py
		cgi_path		cgiFiles/
		cgi_extention	.py
	}
	location /cgi/post {
		root			cgiFiles/
		methodes		POST
		redirection		post.py
		cgi_path		cgiFiles/
		cgi_extention	.py
	}

}